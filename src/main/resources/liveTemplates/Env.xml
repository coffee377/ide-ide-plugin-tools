<templateSet group="Env">
    <template name="ap-name" value="APP_NAME" description="app name" toReformat="true"
              toShortenFQNames="true">
        <context>
            <option name="ENV_KEY" value="true"/>
        </context>
    </template>
    <template name="ap-port" value="APP_PORT" description="app port" toReformat="true"
              toShortenFQNames="true">
        <context>
            <option name="ENV_KEY" value="true"/>
            <option name="ENV_FILE" value="true"/>
        </context>
    </template>
</templateSet>
